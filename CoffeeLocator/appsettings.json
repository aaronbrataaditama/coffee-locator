{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GoogleApiKey": "<GOOGLE_API_KEY>",
  "ConnectionStrings": {
    "sqlConnection": "server=<SQLSERVER_NAME>;database=<DATABASE_NAME>;TrustServerCertificate=True;Integrated Security=true"
  },
  "JwtConfig": {
    "validIssuer": "https://localhost:7003;http://localhost:5003",
    "validAudience": "https://localhost:7003;http://localhost:5003",
    "secret": "<JWT_SECRET>",
    "expiresIn": 10
  }
}
